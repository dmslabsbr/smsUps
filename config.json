{
  "name": "SMS UPS BETA",
  "version": "0.39b",
  "slug": "hassio_sms_ups",
  "description": "Read data from SMS BRASIL UPS",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "url": "https://github.com/dmslabsbr/smsUps",
  "startup": "application",
  "boot": "auto",
  "ports": {
    "8000/tcp": 8000
  },
  "ports_description": {
    "8000/tcp": "Web Interface - Not working yet!"
  },
  "hassio_api": true,
  "homeassistant_api": true,
  "hassio_role": "manager",
  "devicetree": true,
  "devices": ["/dev/ttyUSB0", "/dev/ttyAMA0"],
  "webui": "[PROTO:use_ssl]://[HOST]:[PORT:8000]/",
  "services": [
    "mqtt:need"
  ],
  "ingress": true,
  "ingress_port": 5000,
  "host_network": true,
  "panel_icon": "mdi:cloud",
  "panel_title": "SMS BRASIL UPS",
  "panel_admin": true,
  "options": {
    "UPS_NAME": "SMS",
    "UPS_ID": "01",
    "SMSUPS_SERVER": true,
    "SMSUPS_CLIENTE": true,
    "SMSUPS_FULL_POWER": 1400,
    "PORTA": "/dev/ttyUSB0",
    "allow_shutdown": true,
    "SHUTDOWN_CMD": ["sudo shutdown -h now", "sudo shutdown now", "systemctl poweroff", "sudo poweroff"],
    "USE_SECRETS": false,
    "Long_lived_access_token":"",
    "DEVELOPERS_MODE": false
  },
  "schema": {
    "UPS_NAME": "str",
    "UPS_ID": "str",
    "SMSUPS_SERVER": "bool",
    "SMSUPS_CLIENTE": "bool",
    "SMSUPS_FULL_POWER": "str",
    "PORTA": "str",
    "allow_shutdown": "bool",
    "SHUTDOWN_CMD": "str",
    "USE_SECRETS": "bool",
    "Long_lived_access_token": "str?",
    "DEVELOPERS_MODE": "bool"
  }
}
